  <%= raw File.read(Rails.root.join('app', 'assets', 'images', 'Organigramm.svg')) %>
</div>

<div class="post-it-container">
  <h1 class="fuzzy-bubbles-regular post-it-sponsor-3">Abteilungsleitung</h1>
</div>
<div class="container">
  <div class="row justify-content-center">
    <div class="col-lg-4 col-md-6 col-sm-10">
      <% @leader = Leader.where(funktion: "Abteilungsleitung").first %>

      <%= render partial: "shared/contact_form", locals: { image: @leader.image,
                                                           name: "#{@leader.first_name} #{@leader.last_name} v/o #{@leader.pfadiname.capitalize}",
                                                           funktion: "#{@leader.funktion}",
                                                           email: "#{@leader.email}" } if @leader.present? %>
    </div>
    <div class="col-lg-4 col-md-6 col-sm-10">
      <% @leader = Leader.where(funktion: "Stv. Abteilungsleitung").first %>
      <%= render partial: "shared/contact_form", locals: { image: @leader.image,
                                                           name: "#{@leader.first_name} #{@leader.last_name} v/o #{@leader.pfadiname.capitalize}",
                                                           funktion: "#{@leader.funktion}",
                                                           email: "#{@leader.email}" } if @leader.present? %>
    </div>
  </div>
</div>
